@model LogoutViewModel
<div class="logout-page">
    <div class="page-header">
        <h1>Изход</h1>
    </div>
    <div class="row">
        <div class="col-sm-6">
            <p>Искате ли да излезете от Дари Храна?</p>
            <form asp-action="Logout">
                <input type="hidden" name="logoutId" value="@Model.LogoutId" />
                <fieldset>
                    <div class="form-group">
                        <button class="btn btn-primary">Да</button>
                    </div>
                </fieldset>
            </form>
        </div>
    </div> star
</div>

